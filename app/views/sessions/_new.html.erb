  <div class="col-sm-3 col-xs-8 col-xs-offset-2 col-sm-offset-8 fixMeOnScreen hideAfterCall">
     <h3 class="text-center">Examplon</h3>
       <%=image_tag("477.GIF", alt: "home image",
      width: 62, height: 64, class: "showGif") %>
    <%= form_for(:session, url: login_path,html: {class: 'hideThis'}, remote: true) do |f| %>
          <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= f.label :password %>
      <%= link_to "(forgot password?)", new_password_reset_path %>
      <%= f.password_field :password, class: 'form-control' %>
      
      <%= f.label :remember_me, class: "checkbox inline" do %>
        <%= f.check_box :remember_me %>
        <span>Remember me</span>
      <% end %>
      
      <%= f.submit "Sign in", class: "btn btn-primary" %>
    <% end %>

    <p class="giveMargin pleaseReplace">New user? <%= link_to "Sign up now!",
    signup_path, remote: true %></p>
  </div>